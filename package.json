    "vue-router": "^3.1.6",
    "vue-swipe-actions": "^2.0.0-beta.20",
    "vuex": "^3.3.0",
    "workbox-cli": "^6.4.1"
  },
  "rules": {
    "strict": "off"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
